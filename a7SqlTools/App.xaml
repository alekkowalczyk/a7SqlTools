<Application x:Class="a7SqlTools.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:cv="clr-namespace:a7SqlTools.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/WhistlerBlue.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Fluent;Component/Themes/Windows8/Generic.xaml" />
                <ResourceDictionary Source="Themes/TreeView.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Themes/Templates/ButtonsTemplates.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
            <cv:BoolToVisibility x:Key="b2v"></cv:BoolToVisibility>
            <cv:BoolToVisibilityReverse x:Key="b2vr"></cv:BoolToVisibilityReverse>
            <cv:NegateBool x:Key="nb"></cv:NegateBool>
            <cv:EmptyStringToVisibility x:Key="es2v"></cv:EmptyStringToVisibility>
        </ResourceDictionary>
    </Application.Resources>
</Application>
